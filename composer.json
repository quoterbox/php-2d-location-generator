{
    "name": "quoterbox/php-2d-location-generator",
    "description": "Location Generator from 2d tiles",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "JohnGo Quoterbox",
            "email": "info@quoterbox.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.21",
        "ext-gd": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Backend\\": "src/backend/"
        },
        "files": [
            "src/App/Functions/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/routing": "5.4.x-dev",
        "symfony/http-foundation": "5.4.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
